ontent'>\n\n                {loadedProducts().map(product => (\n                    <div className='products_block'>\n                        <ProductItem product={product} onSubmit={updateProduct} onSubmitDelete={deleteProduct}/>\n                    </div>\n                ))}\n\n            </div>\n        </Fragment>\n    )\n};\n\nfunction mapStateToProps(state) {\n    return {\n        products: state.products\n    };\n}\n\nexport default connect(mapStateToProps, actions)(ProductsList);\n\n","/Users/anastasiabojcuk/mars4rent.zzz.com.ua/hot-dog-front/src/components/InputProduct.js",["36"],"import React from 'react';\nimport ModalInput from \"./ModalInput\";\nimport logo from './img/logo.jpeg';\n\n\nconst InputProduct = ({header}) => {\n\n    return (\n        <div>\n            <div className='products_header'>\n                <div>\n                    <img src={logo} className=\"logo\"/>\n                    <ModalInput header={header}/>\n                </div>\n\n            </div>\n        </div>\n    );\n};\n\nexport default InputProduct;","/Users/anastasiabojcuk/mars4rent.zzz.com.ua/hot-dog-front/src/index.js",[],{"ruleId":"37","severity":1,"message":"38","line":4,"column":5,"nodeType":"39","messageId":"40","endLine":17,"endColumn":6},{"ruleId":"41","severity":1,"message":"42","line":4,"column":5,"nodeType":"43","messageId":"44","endLine":4,"endColumn":12},{"ruleId":"41","severity":1,"message":"45","line":5,"column":5,"nodeType":"43","messageId":"44","endLine":5,"endColumn":12},{"ruleId":"46","replacedBy":"47"},{"ruleId":"48","replacedBy":"49"},{"ruleId":"50","severity":1,"message":"51","line":25,"column":8,"nodeType":"52","endLine":25,"endColumn":10,"suggestions":"53"},{"ruleId":"41","severity":1,"message":"54","line":51,"column":19,"nodeType":"43","messageId":"44","endLine":51,"endColumn":27},{"ruleId":"55","severity":1,"message":"56","line":12,"column":21,"nodeType":"57","endLine":12,"endColumn":55},"default-case","Expected a default case.","SwitchStat